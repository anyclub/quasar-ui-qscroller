      "applicable": [
        "time-range"
      ],
      "default": "['AM', 'PM']",
      "examples": [
        ":end-am-pm-labels=\"['a', 'p']\""
      ]
    },
    "start-hour-interval": {
      "type": [ "String", "Number" ],
      "tsType": "StringOrNumber",
      "category": "content",
      "desc": "Used to create intervals. For instance, if '2', then every other value will be displayed",
      "default": "1",
      "applicable": [
