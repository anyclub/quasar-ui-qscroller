    "start-minute-interval": {
      "type": [ "String", "Number" ],
      "tsType": "StringOrNumber",
      "category": "content",
      "desc": "Used to create intervals. For instance, if '2', then every other value will be displayed",
      "default": "1",
      "applicable": [
        "time-range"
      ],
      "examples": [
        ":start-minute-interval=\"2\""
      ]
    },
    "end-minute-interval": {
      "type": [ "String", "Number" ],
      "tsType": "StringOrNumber",
